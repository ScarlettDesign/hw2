
function setup() {
    createCanvas(400, 400);
}

function draw() {
    background(0);
	
	fill(200,200,170)
	quad(200,10,width-10,200,200,height-10,10,200)
	
	fill(0)
	noStroke()
	rect(0,115,width,8)
	rect(105,0,8,height)
	rect(315,0,15,width)
	rect(0,320,width,15)
	
	stroke(200,200,170)
	strokeWeight(5)
	line(200,10,width-10,200)
	line(width-10,200,200,height-10)
	line(200,height-10,10,200)
	line(10,200,200,10)
}
